@model MoostBrand.DAL.StockAllocation
@{
    ViewBag.Title = "Delete";
}
<div class="panel-heading">
    <h4 class="panel-title">Requisition Details</h4>
    <hr />
</div>
<div class="panel-body">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-md-2">Date</label>
            <div class="col-md-6">
                <label class="custom-label">
                    @Html.DisplayFor(model => model.SADate)
                </label>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Time</label>
            <div class="col-md-6">
                <label class="custom-label">
                    @Html.DisplayFor(model => model.Time)
                </label>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Receiving ID</label>
            <div class="col-md-6">
                <label class="custom-label">
                    @Html.DisplayFor(model => model.Receiving.ReceivingID)
                </label>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2">Remarks</label>
            <div class="col-md-6">
                <label class="custom-label">
                    @Html.DisplayFor(model => model.Remarks)
                </label>
            </div>
        </div>
        @using (Html.BeginForm())
        {
            <div class="form-group">
                <div class="col-md-offset-2 col-md-6">
                    <input type="submit" value="Delete" class="btn btn-dark-red" />
                </div>
            </div>
        }
    </div>
</div>